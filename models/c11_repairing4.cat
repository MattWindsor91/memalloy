(* Models from paper "Repairing SC in C/C++11" by Lahav et al. *)

"C"
include "c11_base.cat"
		       
(* Allowing elimination of SC accesses: *)
let rb = [R]; rf^-1; co
let shsb = (po \ loc); hb; (po \ loc)
let eco = (rf | co | rb)+
let Fhb = [F];hb
let hbF = hb;[F]
acyclic [sc]; Fhb?; (po | shsb | eco); hbF?; [sc] as Srepaired4
